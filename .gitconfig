[user]
  email = antcar1992@gmail.com
  name = Anthony Caron
# Set work specific email
# [includeIf "gitdir:~/dev/"]
#   path = ~/dev/.gitconfig
[include]
  path = ~/dotfiles/private.gitconfig
[alias]
  br = branch
  ci = commit
  co = checkout
  f = fetch
  st = status
[push]
  autoSetupRemote = true
[pull]
  rebase = true
[rebase]
  autostash = true
[core]
  ignoreCase = false
  editor = nvim
[init]
  defaultBranch = main
[credential]
	helper = store
